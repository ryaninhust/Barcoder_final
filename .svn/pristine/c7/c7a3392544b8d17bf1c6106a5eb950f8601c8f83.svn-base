<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/personalinfo_penel1"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" >

        <LinearLayout
            android:id="@+id/personalinfo_photo"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/tile_bg"
            android:orientation="vertical" >

        </LinearLayout>

        <TextView
            android:id="@+id/personalinfo_lable_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/linearLayout1"
            android:layout_marginLeft="14dp"
            android:layout_marginTop="14dp"
            android:layout_toRightOf="@+id/personalinfo_photo"
            android:text="姓名"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/personalinfo_lable_uid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/personalinfo_lable_name"
            android:layout_below="@+id/personalinfo_lable_name"
            android:text="学号" />

        <TextView
            android:id="@+id/personalinfo_lable_scool"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/personalinfo_lable_uid"
            android:layout_below="@+id/personalinfo_lable_uid"
            android:text="学院专业班级" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/personalinfo_pennel_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="120dp" >

        <TextView
            android:id="@+id/personalinfo_lable_phone"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="手机"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/personalinfo_lable_QQ"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/personalinfo_lable_phone"
            android:text="QQ"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/personalinfo_lable_Email"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/personalinfo_lable_QQ"
            android:text="Email"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/personalinfo_lable_dorm"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/personalinfo_lable_Email"
            android:text="宿舍"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/personalinfo_lable_shuoshuo"
            android:layout_width="match_parent"
            android:layout_height="75dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/personalinfo_lable_dorm"
            android:text="个人签名"
            android:textAppearance="?android:attr/textAppearanceMedium" />

    </RelativeLayout>

</RelativeLayout>